{
  "name": "pocds-icons",
  "version": "0.0.0",
  "main": "build/icons.js",
  "module": "build/icons.js",
  "license": "MIT",
  "scripts": {
    "start": "node_modules/.bin/npm-run-all copy svgo build",
    "build": "node ./src/scripts/build.js",
    "svgo": "node_modules/.bin/svgo -f ./build/assets",
    "copy": "node_modules/.bin/copyfiles --up 1 \"./src/assets/*\" \"./build\"",
    "clean": "rm -rf build",
    "import-icons": "node_modules/.bin/export-icons"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "svgo": "^2.6.0"
  },
  "dependencies": {
    "figma-export-icons": "^1.5.0"
  }
}
